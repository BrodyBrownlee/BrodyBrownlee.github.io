{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "^/audio/(.*)",
      "dest": "/audio/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
